{
	"name": "parkhub-challenge-backend",
	"version": "1.0.0",
	"main": "./dist/app.js",
	"module": "./dist/app.mjs",
	"types": "./dist/app.d.ts",
	"type": "module",
	"exports": {
		"require": {
			"types": "./dist/app.d.cts",
			"default": "./dist/app.cjs"
		},
		"import": {
			"types": "./dist/app.d.mts",
			"default": "./dist/app.mjs"
		}
	},
	"scripts": {
		"dev": "tsx watch --clear-screen=false src/app.ts",
		"build": "pkgroll",
		"start": "node dist/app.js"
	},
	"dependencies": {
		"dotenv": "^16.4.5",
		"envalid": "^8.0.0",
		"express": "^4.21.1",
		"kysely": "^0.27.4",
		"pg": "^8.13.0",
		"winston": "^3.15.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.3",
		"@types/express": "^5.0.0",
		"@types/node": "^22.7.5",
		"@types/pg": "^8.11.10",
		"pkgroll": "^2.5.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.3"
	}
}
